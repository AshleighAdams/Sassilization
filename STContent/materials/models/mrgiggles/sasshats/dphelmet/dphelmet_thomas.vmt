"VertexlitGeneric"
{
	"$baseTexture" "models/mrgiggles/sasshats/dphelmet/dphelmet_thomas"
	"$selfillum" "1"

	"$envmap" "env_cubemap"
	"$envmapmask" "models/mrgiggles/sasshats/dphelmet/dphelmet_thomas_envmap"
	"$envmaptint" "[0.35 0.35 0.35]"

	"Proxies"
	{
		 "AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" "4"
     		 }
	}
}

